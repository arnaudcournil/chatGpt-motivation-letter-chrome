{
    "manifest_version": 2,
    "name": "Mon Extension",
    "description": "Extension test Chrome",
    "version": "1.0",
    "permissions": [
     "*://*/*",
     "activeTab",
     "scripting",
     "clipboardWrite"
    ],
    "content_scripts": [
     {
      "matches": ["*://*/*"],
      "js": ["monscript.js"]
     }
    ],
    "browser_action": {

     "default_popup": "popup.html"
    }
}